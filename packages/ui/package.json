{
  "name": "@scxfe/ui",
  "version": "0.0.3",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/shawicx/scx-core#readme",
  "bugs": {
    "url": "https://github.com/shawicx/scx-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shawicx/scx-core.git"
  },
  "funding": "https://github.com/sponsors/shawicx",
  "license": "ISC",
  "author": "dev.rehab498@passinbox.com",
  "sideEffects": false,
  "main": "./lib/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "build": "father build",
    "lint": "eslint . && stylelint \"**/*.{css,less,scss}\"",
    "lint:fix": "prettier --write . && eslint --fix . && stylelint --fix \"**/*.{css,less,scss}\""
  },
  "prettier": "prettier-config-ali",
  "eslintConfig": {
    "extends": [
      "ali/typescript/react",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "simple-import-sort/imports": [
        "error",
        {
          "groups": [
            [
              "^@?\\w"
            ],
            [
              "^"
            ],
            [
              "^\\."
            ],
            [
              "^.+\\u0000$"
            ],
            [
              "^\\u0000"
            ]
          ]
        }
      ],
      "simple-import-sort/exports": "error"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-ali",
      "stylelint-prettier/recommended"
    ]
  },
  "dependencies": {
    "@scxfe/util": "workspace:^",
    "ahooks": "^3.8.0"
  },
  "devDependencies": {
    "@amap/amap-jsapi-types": "^0.0.15",
    "@scxfe/ts-config": "workspace:^",
    "@turbo/gen": "^1.13.2",
    "@types/eslint": "^8.56.7",
    "@types/node": "^20.12.6",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "eslint": "^8.57.0",
    "eslint-config-ali": "^15.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "father": "^4.4.0",
    "prettier": "^3.2.5",
    "prettier-config-ali": "^1.3.1",
    "react": "^18.2.0",
    "stylelint": "^16.3.1",
    "stylelint-config-ali": "^2.1.1",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
