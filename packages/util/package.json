{
  "name": "@scxfe/util",
  "version": "0.0.3",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/shawicx/scx-core#readme",
  "bugs": {
    "url": "https://github.com/shawicx/scx-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shawicx/scx-core.git"
  },
  "funding": "https://github.com/sponsors/shawicx",
  "license": "ISC",
  "author": "dev.rehab498@passinbox.com",
  "sideEffects": false,
  "main": "./lib/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "umd",
    "lib"
  ],
  "scripts": {
    "build": "father build",
    "lint": "eslint .",
    "lint:fix": "prettier --write . && eslint --fix ."
  },
  "prettier": "prettier-config-ali",
  "eslintConfig": {
    "extends": [
      "ali/typescript/react",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "simple-import-sort/imports": [
        "error",
        {
          "groups": [
            [
              "^@?\\w"
            ],
            [
              "^"
            ],
            [
              "^\\."
            ],
            [
              "^.+\\u0000$"
            ],
            [
              "^\\u0000"
            ]
          ]
        }
      ],
      "simple-import-sort/exports": "error"
    }
  },
  "devDependencies": {
    "@scxfe/ts-config": "workspace:^",
    "@types/gradient-string": "^1.1.6",
    "eslint": "^8.57.0",
    "eslint-config-ali": "^15.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "father": "^4.4.0",
    "prettier": "^3.2.5",
    "prettier-config-ali": "^1.3.1"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "gradient-string": "^2.0.2",
    "ora": "^8.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
