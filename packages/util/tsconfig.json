{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "scxfe Util",
  "extends": "../../packages/ts-config/library.json",
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "lib", "umd", "**/*.test.*", "**/*.spec.*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "target": "ES2019",
    "module": "ESNext",
    "lib": ["ES2019", "DOM"]
  }
}
